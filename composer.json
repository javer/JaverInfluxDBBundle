{
    "name": "javer/influxdb-odm-bundle",
    "type": "symfony-bundle",
    "description": "Provides integration of InfluxDB ODM with Symfony",
    "keywords": ["influxdb", "doctrine", "odm", "symfony"],
    "homepage": "https://github.com/javer/JaverInfluxDBBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "doctrine/annotations": "^1.12",
        "javer/influxdb-odm": "^1.3",
        "symfony/config": "^4.4 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.0 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.0 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.0 || ^6.0",
        "symfony/service-contracts": "^1.0 || ^2.0 || ^3.0",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "squizlabs/php_codesniffer": "3.6.*",
        "swivl/php-coding-standard": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Javer\\InfluxDB\\Bundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
