{
    "name": "javer/influxdb-odm-bundle",
    "type": "symfony-bundle",
    "description": "Provides integration of InfluxDB ODM with Symfony",
    "keywords": ["influxdb", "doctrine", "odm", "symfony"],
    "homepage": "https://github.com/javer/JaverInfluxDBBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": "^1.0",
        "javer/influxdb-odm": "^1.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/service-contracts": "^1.0 || ^2.0",
        "symfony/yaml": "^4.4 || ^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.*",
        "swivl/php-coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Javer\\InfluxDB\\Bundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
